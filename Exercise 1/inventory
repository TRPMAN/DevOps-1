all :
  hosts:
    web01:
      ansible_host: 172.31.90.203
      # ansible_user: ec2-user
      # ansible_ssh_private_key_file: ansible-client-key.pem
    web02:
      ansible_host: 172.31.82.67
      # ansible_user: ec2-user
      # ansible_ssh_private_key_file: ansible-client-key.pem
    db01:
      ansible_host: 172.31.88.202
      # ansible_user: ec2-user
      # ansible_ssh_private_key_file: ansible-client-key.pem
  # Group of Hosts
  children:
    webservers:
      hosts:
        web01:
        web02:
    dbservers:
      hosts:
        db01:
    # Group of Group
    dc_oregon:
      children:
        webservers:
        dbservers: 
      # Set Variable at the top of the dc-oregon
      vars:
        ansible_user: ec2-user
        ansible_ssh_private_key_file: ansible-client-key.pem    